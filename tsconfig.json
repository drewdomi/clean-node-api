{
  "compilerOptions": {
    "esModuleInterop": true,
    "moduleResolution": "Bundler",
    "removeComments": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "noImplicitAny": true,
    "target": "ES2023",
    "allowJs": false,
    "isolatedModules": true,
    "strict": true,
    "noUncheckedIndexedAccess": true,
    "noImplicitOverride": true,
    "module": "ES2022",
    "outDir": "dist",
    "lib": ["es2022"]
  },
  "include": ["**/*.ts", "**/*.tsx", "src/utils/dasdas"],
  "exclude": ["node_modules"]
}
